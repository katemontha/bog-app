<% @creatures.each do |creature| %>
    <div class="row">
        <div class="col s12 m5">
            <div class="card-panel teal">
                <p>
                    <strong>Name:</strong>
                    <%= creature.name %><br>
                        <strong>Description:</strong>
                        <%= creature.description %>
                </p>
            </div>
        </div>
    </div>
    <% end %>